{
  "$schema": "https://docs.renovatebot.com/renovate-schema.json",
  "extends": [
    "config:recommended"
  ],
  "timezone": "Asia/Tokyo",
  "prHourlyLimit": 1,
  "automerge": false,
  "rangeStrategy": "bump",
  "labels": [
    "dependencies"
  ],
  "packageRules": [
    {
      "matchUpdateTypes": [
        "patch",
        "minor"
      ]
    },
    {
      "excludePackageNames": [
        "kotlin"
      ]
    }
  ]
}
